<header id="header">
  <div class="headTtl clearfix">
    <a href="<%- url_for("/") %>">
    <% if (theme.customize.logo && theme.customize.logo.enabled) { %>
      <% if (theme.customize.gravatar.email && theme.customize.logo.gravatar) { %>
        <div id="logo" style="background-image: url(<%- gravatar(theme.customize.gravatar.email) %>);"></div>
      <% } else { %>
        <div id="logo" style="background-image: url(<%- url_for(theme.customize.logo.url) %>);"></div>
        <div id="logo" style="background-image: url(<%- url_for(theme.customize.logo2.url) %>);"></div>
      <% } %>
    <% } %>
      <div id="title">
        <h1><%= config.title %></h1>
      </div>
    </a>
  </div>
  <div id="nav">
    <ul>
      <li class="icon">
        <a href="#"><i class="fa fa-bars fa-2x"></i></a>
      </li>
      <% for (var i in site.data.menu) { %>
        <li><a href="<%- url_for(site.data.menu[i]) %>"><%= i %></a></li>
      <% } %>
    </ul>
  </div>
</header>
